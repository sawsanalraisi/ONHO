@model HydrographicOffice.Models.NavigationWVm

@{
    ViewData["Title"] = "Delete";
    Layout = "_AdminLayout";
}





<link href="~/Admin/sty/Style-Page.css" rel="stylesheet" />

<h1 class="txtColorBlue text-center"><mark class="highlight">Navigation Warning </mark></h1>

<hr />
<div class="container  mb-5">
    <div class="row d-flex align-items-center justify-content-center ">
        <div class="col-md-10 shadow-lg p-3 mb-5 bg-body rounded mt-5">



            <h3>Are you sure you want to delete this?</h3>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.NavWarnDesc)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.NavWarnDesc)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.NavFileName)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.NavFileName)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.WarnNumber)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.WarnNumber)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.WaringDate)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.WaringDate)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Status)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Status)
                </dd>

                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.UploadeFile)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.UploadeFile)
                </dd>


            </dl>




           

            <form asp-action="DeleteConfirmed">
                <input type="hidden" asp-for="Id" />

                <div class="form-input">
                    <input type="submit" value="Delete" class="btn btn-danger" style="width:100%;font-size:22px;" />
                </div>

                <div class="form-input">
                    <a asp-action="Index"><i class="fas fa-arrow-left"></i> &nbsp;Back to List</a>
                </div>





            </form>




        </div>
    </div>
</div>
