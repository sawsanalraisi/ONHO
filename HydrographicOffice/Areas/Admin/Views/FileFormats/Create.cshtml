@model Hydro.DAL.Entities.FileFormat
@inject CultureLocalizer  _Localizer


@{
    ViewData["Title"] = "Create";
    Layout = "_AdminLayout";
}


<div class="container mt-5">
    <div class="row d-flex align-items-center justify-content-center">
        <div class="col-md-10 ">

            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>



                <h1 class="txtColorBlue text-center"><mark class="highlight">File Format </mark></h1>

                <hr />




                <div class="form-input">
                    <input asp-for="FileType" class="form-control form-controlNonSpace" placeholder="File Type" />
                    <span asp-validation-for="FileType" class="text-danger"></span>
                </div>




                <div class="form-input">
                    <button type="submit" class="btn mt-4 signup backBlue text-white" style="width:100%;font-size:22px;"> @_Localizer.Text("Save") </button>
                </div>

            </form>

        </div>
    </div>
</div>






@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}




